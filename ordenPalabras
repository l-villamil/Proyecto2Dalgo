def ordenPalabra(palabra1,palabra2,i):
    centinela=-1
    while i<len(palabra1) and centinela==-1:
        if palabra1[i]!=palabra2[i]:
            centinela=i
        else:
            i+=1
            centinela=ordenPalabra(palabra1,palabra2,i)
    return centinela
#print(ordenPalabra("ab","mn",0))